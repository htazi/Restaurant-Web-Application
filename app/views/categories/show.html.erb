<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @category.name %>
</p>

<table>
    <strong>FoodItems:</strong>
    <% @category.food_items.each do |food_item| %>
      <tr>
        <td><%= link_to food_item.name, food_item %></td>
      </tr>
      <% end %>
</table>


<%= link_to 'Edit', edit_category_path(@category) %> |
<%= link_to 'Back', categories_path %>
